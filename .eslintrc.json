{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["prettier", "airbnb-base"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "eslint-plugin-prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-restricted-syntax": "off"
    }
}